<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>CEEBOX#SHOP @ViewBag.Title</title>

    <link rel="stylesheet" href="~/Content/Styles/reset.css" />
    <link rel="stylesheet" href="~/Content/Styles/Layout/layout.css" />

    <link href="https://fonts.googleapis.com/css?family=Passion+One" rel="stylesheet">
</head>
<body>
    <header>
        <div class="container">
            <div class="header_top_bar">
                <div class="search left">
                    <form action="">
                        <input type="submit" value="">
                        <input type="search" placeholder="Search">
                    </form>
                </div>
                <div class="cart right">
                    <div class="clearfix">
                        <div class="left_cart left">
                            <img src="/Content/Images/shopping-cart.png" alt="">
                            <span>Cart(0)</span>
                        </div>
                        <div class="check_out right">
                            <span>Check out</span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="title">
                <a href="#">
                    <img src="/Content/Images/logo.png" alt="">
                </a>
            </div>
        </div>
    </header>
    <section id="body">
        <div class="container clearfix">
            <section id="menu" class="left">
                <nav>
                    <ul>
                        <li>
                            <img src="data:image/png;base64,">
                            <span>Home</span>
                        </li>
                        <li>
                            <img src="data:image/png;base64,">
                            <span>Accessories</span>
                        </li>
                        <li>
                            <img src="data:image/png;base64,">
                            <span>Denim</span>
                        </li>
                        <li>
                            <img src="data:image/png;base64,">
                            <span>Footwear</span>
                        </li>
                        <li>
                            <img src="data:image/png;base64,">
                            <span>Outerwear</span>
                        </li>
                        <li>
                            <img src="data:image/png;base64,">
                            <span>Pants</span>
                        </li>
                        <li>
                            <img src="data:image/png;base64,">
                            <span>Shirts</span>
                        </li>
                        <li>
                            <img src="data:image/png;base64,">
                            <span>T-Shirts</span>
                        </li>
                        <li>
                            <img src="data:image/png;base64,">
                            <span>Shorts</span>
                        </li>
                    </ul>
                </nav>
            </section>
            <section id="content" class="right">
                @RenderBody()
            </section>
        </div>
    </section>
</body>
</html>